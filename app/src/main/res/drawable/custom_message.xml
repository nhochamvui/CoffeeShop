<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

<item>
    <shape>
        <solid android:color="#ffffff" />
    </shape>
</item>
<item>
    <shape>
        <stroke
            android:width="1dip"
            android:color="#2690A0AF" />

        <solid
            android:color="#00FFFFFF"
